<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.DEV" parent="Theme.MaterialComponents.Light.Bridge">
        <item name="android:colorBackground">#d2d6da</item>
        <item name="colorSurface">#ffffff</item>
        <item name="colorOnBackground">#000000</item>
        <item name="colorOnSurface">#000000</item>
        <item name="colorSecondary">#000000</item>
        <item name="colorSecondaryVariant">#000000</item>
        <item name="colorOnSecondary">#ffffff</item>
        <item name="colorPrimary">#ffffff</item>
        <item name="colorOnPrimary">#000000</item>
        <item name="colorPrimaryVariant">#d2d6da</item>
        <item name="colorError">#dc3545</item>
        <item name="colorOnError">#ffffff</item>

        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>

    <style name="Theme.DEV.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.DEV.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.DEV.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>